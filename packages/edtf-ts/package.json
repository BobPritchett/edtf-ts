{
  "name": "@edtf-ts",
  "version": "0.2.0",
  "description": "Modern TypeScript implementation of Extended Date/Time Format (EDTF) with temporal reasoning",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "edtf",
    "extended-date-time-format",
    "date",
    "time",
    "temporal",
    "interval",
    "allen-relations",
    "comparison",
    "formatting",
    "parsing",
    "iso-8601",
    "library-of-congress"
  ],
  "author": "Bob Pritchett",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
