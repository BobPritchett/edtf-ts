{
  "name": "@edtf-ts/compare",
  "version": "0.1.0",
  "description": "Advanced comparison, Allen relations, and database preparation for EDTF temporal values",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "edtf",
    "extended-date-time-format",
    "temporal",
    "comparison",
    "allen-relations",
    "database",
    "bigint"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@edtf-ts/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
