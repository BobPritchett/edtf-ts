{
  "name": "@edtf-ts/core",
  "version": "0.1.0",
  "description": "Core EDTF parsing and types",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest",
    "lint": "eslint src"
  },
  "keywords": [
    "edtf",
    "date",
    "parser",
    "typescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "vitest": "^1.0.0"
  }
}