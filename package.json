{
  "name": "edtf-ts",
  "version": "0.4.2",
  "private": true,
  "description": "Modern TypeScript implementation of EDTF (Extended Date/Time Format)",
  "scripts": {
    "build": "pnpm -r run build",
    "test": "pnpm -r run test",
    "lint": "pnpm -r run lint",
    "dev": "pnpm -r --parallel run dev",
    "docs:dev": "pnpm --filter @edtf-ts/docs dev",
    "docs:build": "pnpm --filter @edtf-ts/docs build",
    "docs:preview": "pnpm --filter @edtf-ts/docs preview"
  },
  "keywords": [
    "edtf",
    "date",
    "time",
    "parser",
    "typescript",
    "iso8601",
    "natural-language"
  ],
  "author": "Bob Pritchett",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.27",
    "eslint": "^8.57.1",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
